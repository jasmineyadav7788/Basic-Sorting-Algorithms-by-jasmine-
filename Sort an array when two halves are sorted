// Java program to Merge two sorted halves of
// array Into Single Sorted Array
import java.io.*;
import java.util.*;

class GFG {

	static void mergeTwoHalf(int[] A, int n)
	{
		// Sort the given array using sort STL
		Arrays.sort(A);
	}

	// Driver code
	static public void main(String[] args)
	{
		int[] A = { 2, 3, 8, -1, 7, 10 };
		int n = A.length;
		mergeTwoHalf(A, n);

		// Print sorted Array
		for (int i = 0; i < n; i++)
			System.out.print(A[i] + " ");
	}
}


